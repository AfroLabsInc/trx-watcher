    "url": "git+https://github.com/johnexzy/web3gg.git"
  },
  "keywords": [
    "wallet",
    "bots",
    "ethereum",
    "blockchain"
  ],
  "author": "obajohn75@gmail.com",
  "license": "MIT",
  "dependencies": {
    "alchemy-sdk": "^2.8.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.4",
    "express": "^4.19.2"
  },
  "bugs": {
    "url": "https://github.com/afrolabsinc/watcher/issues"
  },
  "homepage": "https://github.com/afrolabsinc/watcher#readme",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.13",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "cz-emoji": "^1.3.1",
    "nodemon": "^2.0.22",
    "typescript": "^4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
